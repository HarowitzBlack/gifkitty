{

  "manifest_version": 2,
  "name": "gifkitty",
  "version": "1.0",

  "description": "gifs everywhere",

  "content_scripts": [
    {
      "matches": ["*://*.producthunt.com/*"],
      "js": ["jquery-3.3.1.min.js","caret.js","app.js"],
      "css": ["main.css"]
    }
  ],
  "permissions": [
  "*://api.giphy.com/*","webRequest"
  ]

}
